# 环境配置示例文件
# 复制此文件为 .env 并修改相应配置

# 应用环境
# development: 开发环境
# production: 生产环境
NODE_ENV=development

# API 基础地址
VUE_APP_API_BASE_URL=http://localhost:8000/api/v1

# 文件上传地址
VUE_APP_UPLOAD_URL=http://localhost:8000/api/v1/upload

# 静态资源地址
VUE_APP_STATIC_URL=http://localhost:8000/static

# 企业微信群机器人 Webhook（用于消息通知）
VUE_APP_WECHAT_WEBHOOK=https://qyapi.weixin.qq.com/cgi-bin/webhook/send?key=YOUR_KEY

# 应用版本
VUE_APP_VERSION=3.1.23

# 是否启用调试模式
VUE_APP_DEBUG=true

# 是否启用 Mock 数据
VUE_APP_MOCK=false

# 请求超时时间（毫秒）
VUE_APP_REQUEST_TIMEOUT=30000

# 分页默认每页数量
VUE_APP_PAGE_SIZE=20

# 文件上传大小限制（MB）
VUE_APP_MAX_FILE_SIZE=10

# 是否启用日志
VUE_APP_ENABLE_LOG=true
